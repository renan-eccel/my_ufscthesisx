	keywords = {ad hoc networks, assignment policy, autonomous vehicles, Centralized control, centralized controller, control policies, Delay, delivery locations, delivery problem, Dial-a-ride problem ({DARP}), dynamic pickup and delivery problem ({DPDP}), dynamic pickup problem, dynamic traveling repair-person problem ({DTRP}), Infinite horizon, information constraints, less-than-truckload ({LTL}), message pickup, Mobile ad hoc networks, mobile networks, Mobile robots, Optimal control, Poisson process, Remotely operated vehicles, Routing, stochastic processes, traffic control, unmanned aerial vehicle ({UAV}), Unmanned aerial vehicles, Vehicle dynamics, wireless ad hoc networks},
	file = {IEEE Xplore Abstract Record:/home/renan-eccel/Zotero/storage/V5VM5Y54/4610036.html:text/html;Waisanen et al. - 2008 - A Dynamic Pickup and Delivery Problem in Mobile Ne.pdf:/home/renan-eccel/Zotero/storage/YMXERR7N/Waisanen et al. - 2008 - A Dynamic Pickup and Delivery Problem in Mobile Ne.pdf:application/pdf}
