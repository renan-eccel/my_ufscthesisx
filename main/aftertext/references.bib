	number = {2},
	journaltitle = {International Journal on Artificial Intelligence Tools},
	author = {Li, Haibing and Lim, Andrew},
	date = {2003-06},
	file = {Li and Lim - 2003 - A Metaheuristic for the Pickup and Delivery Proble.pdf:/home/renan-eccel/Zotero/storage/4ZR35SVH/Li and Lim - 2003 - A Metaheuristic for the Pickup and Delivery Proble.pdf:application/pdf}
