	file = {IEEE Xplore Abstract Record:/home/renan-eccel/Zotero/storage/PV2YHQ2R/6394135.html:text/html;IEEE Xplore Full Text PDF:/home/renan-eccel/Zotero/storage/VZMTX6RS/Lon and Holvoet - 2012 - RinSim A Simulator for Collective Adaptive System.pdf:application/pdf}
