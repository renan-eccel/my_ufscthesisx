	langid = {english},
	keywords = {to-read, waiting strategies},
	file = {Full Text PDF:/home/renan-eccel/Zotero/storage/8QTAUFTD/Vonolfen and Affenzeller - 2016 - Distribution of waiting time for dynamic pickup an.pdf:application/pdf;Snapshot:/home/renan-eccel/Zotero/storage/PPPGIEBP/s10479-014-1683-6.html:text/html;Vonolfen and Affenzeller - 2016 - Distribution of waiting time for dynamic pickup an.pdf:/home/renan-eccel/Zotero/storage/XHMCVEGJ/Vonolfen and Affenzeller - 2016 - Distribution of waiting time for dynamic pickup an.pdf:application/pdf}
